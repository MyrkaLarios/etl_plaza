<h2>Estancia</h2>
<div style='display: flex; flex-direction: column;'>
  <% tipos_empleado.each do |e| %>
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p><%= e.fechainicio %></p>
      <p><%= e.fechafin %></p>
      <p><%= e.horainicio %></p>
      <p><%= e.horafin %></p>
      <p><%= e.duracion %></p>
      <p><%= e.subtotal %></p>
      <p><%= e.total %></p>
      <p><%= e.placa %></p>
      <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.ESTANCIA' } %>
    </div>
  <% end %>
</div>