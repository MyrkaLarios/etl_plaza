<h2>Horario</h2>
<div style='display: flex; flex-direction: column;'>
  <% horario.each do |e| %>
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p><%= e.horainicio %></p>
      <p><%= e.horafin %></p>
      <p><%= e.fechainicio %></p>
      <p><%= e.fechafin %></p>
      <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.HORARIO' } %>
    </div>
  <% end %>
</div>