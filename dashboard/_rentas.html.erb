<h2>Rentas</h2>
<div style='display: flex; flex-direction: column;'>
  <% tipos_empleado.each do |e| %>
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p><%= e.fechacobro %></p>
      <p><%= e.statusretraso %></p>
      <p><%= e.statuspagado %></p>
      <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.RENTAS' } %>
    </div>
  <% end %>
</div>