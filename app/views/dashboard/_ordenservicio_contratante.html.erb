<div class="card-header" id="heading32">
  <h2 class="mb-0">
    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse32" aria-expanded="false" aria-controls="collapse32">
      + Orden Servicio - Contratante
    </button>
  </h2>
</div>

<div id="collapse32" class="collapse" aria-labelledby="heading32" data-parent="#accordion">
  <div class="card-body">
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p class="border-bottom" id="top">Id</p>
      <p class="border-bottom" id="top">Nombre</p>
      <p class="border-bottom" id="top">Giro</p>
      <p class="border-bottom" id="top">Estado</p>
      <div class="border-bottom" id="top" style='width: 180px; margin-bottom: 1rem'></div>
    </div>

    <div style='display: flex; flex-direction: column;'>
      <% ordenservicio_contratante.each do |e| %>
        <div style='display: flex; justify-content: space-between;' class='my-2'>
          <p><%= e.id %></p>
          <p><%= e.costo %></p>
          <p><%= e.tiempo %></p>
          <p><%= e.id_ordenservicio %></p>
          <p><%= e.id_contratante %></p>
          <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.ORDENSERVICIO_CONTRATANTE' } %>
        </div>
      <% end %>
    </div>
  </div>
</div>

