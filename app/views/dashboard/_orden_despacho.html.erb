<div class="card-header" id="heading18">
  <h2 class="mb-0">
    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse18" aria-expanded="false" aria-controls="collapse18">
      + Orden de Despacho
    </button>
  </h2>
</div>

<div id="collapse18" class="collapse" aria-labelledby="heading18" data-parent="#accordion">
  <div class="card-body">
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p class="border-bottom" id="top">Id</p>
      <p class="border-bottom" id="top">Cantidad Recibida</p>
      <p class="border-bottom" id="top">Fecha Entrega</p>
      <p class="border-bottom" id="top">Id Solicitud</p>
      <% if current_user.user_type != 1 %>
        <div class="border-bottom" id="top" style='width: 180px; margin-bottom: 1rem'></div>
      <% end %>
    </div>

    <div style='display: flex; flex-direction: column;'>
      <% orden_despacho.each do |e| %>
        <div style='display: flex; justify-content: space-between;' class='my-2'>
          <p><%= e.id %></p>
          <p><%= e.fechaentrega %></p>
          <p><%= e.id_solicitud %></p>
          <% if current_user.user_type != 1 %>
            <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.ORDEN_DESPACHO' } %>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>
