<h2>Ganancias</h2>
<div style='display: flex; flex-direction: column;'>
  <% ganancias.each do |e| %>
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p><%= e.ingresos %></p>
      <p><%= e.egresos %></p>
      <p><%= e.fechacorte %></p>
      <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.GANANCIAS' } %>
    </div>
  <% end %>
</div>
