<div class="card-header" id="heading33">
  <h2 class="mb-0">
    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse33" aria-expanded="false" aria-controls="collapse33">
      + Área Servicios
    </button>
  </h2>
</div>

<div id="collapse33" class="collapse" aria-labelledby="heading33" data-parent="#accordion">
  <div class="card-body">
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p class="border-bottom" id="top">Id</p>
      <p class="border-bottom" id="top">Periodicidad</p>
      <p class="border-bottom" id="top">Id Servicio</p>
      <p class="border-bottom" id="top">Id Tipo Área</p>
      <% if current_user.user_type != 1 %>
        <div class="border-bottom" id="top" style='width: 180px; margin-bottom: 1rem'></div>
      <% end %>
    </div>

    <div style='display: flex; flex-direction: column;'>
      <% areas_servicios.each do |e| %>
        <div style='display: flex; justify-content: space-between;' class='my-2'>
          <p><%= e.id %></p>
          <p><%= e.periodicidad %></p>
          <p><%= e.id_servicio %></p>
          <p><%= e.id_tipoarea %></p>
          <% if current_user.user_type != 1 %>
            <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.AREA_SERVICIO' } %>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>

