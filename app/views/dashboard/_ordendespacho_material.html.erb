<div class="card-header" id="heading19">
  <h2 class="mb-0">
    <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse19" aria-expanded="false" aria-controls="collapse19">
      + Orden Despacho Materiales
    </button>
  </h2>
</div>

<div id="collapse19" class="collapse" aria-labelledby="heading19" data-parent="#accordion">
  <div class="card-body">
    <div style='display: flex; justify-content: space-between;' class='my-2'>
      <p class="border-bottom" id="top">Id</p>
      <p class="border-bottom" id="top">Cantidad Recibida</p>
      <p class="border-bottom" id="top">Fecha Caducidad</p>
      <p class="border-bottom" id="top">Id Orden Despacho</p>
      <div class="border-bottom" id="top" style='width: 180px; margin-bottom: 1rem'></div>
    </div>

    <div style='display: flex; flex-direction: column;'>
      <% ordendespacho_material.each do |e| %>
        <div style='display: flex; justify-content: space-between;' class='my-2'>
          <p><%= e.id %></p>
          <p><%= e.cantidadrecibida %></p>
          <p><%= e.fechadecaducidad %></p>
          <p><%= e.id_ordendespacho %></p>
          <%= render partial: "dashboard/links", locals: { e: e, table: 'dbo.ORDENDESPACHO_MATERIAL' } %>
        </div>
      <% end %>
    </div>
  </div>
</div>
